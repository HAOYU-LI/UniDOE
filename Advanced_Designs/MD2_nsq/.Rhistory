for(file in tmp_nn){
tryCatch({
tmp_name = sub(".csv","", sub("_[0-9]*","_s", sub(CRIT_TYPE,"",file) ) )
file_path = file.path(PATH_TO_FILE,file)
dir_path = file.path(PATH_TO_DIR,tmp_name)
